{
  "name": "velocity-api",
  "version": "2.0.0",
  "description": "A super fast API Wrapper for Perspective.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:code": "tateru",
    "build:exports": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:types": "shx cp src/index.d.ts dist/index.d.ts",
    "prepare": "yarn build"
  },
  "keywords": [
    "perspective",
    "api",
    "perspective api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axisiscool/velocity.git"
  },
  "author": "Axis (axis@axis.lol)",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/axisiscool/velocity/issues"
  },
  "homepage": "https://github.com/axisiscool/velocity#readme",
  "dependencies": {
    "petitio": "^1.3.2",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.6.1",
    "gen-esm-wrapper": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "tateru": "^0.0.9"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
